<%= form_with model: @eatery, local: true do |f| %>

    <h2><%= f.label :name %><h2>
    <p><%= f.text_field :name %></p>

    <h2><%= f.label :location %><h2>
    <p><%= f.text_field :location %></p>

    <h2><%= f.label :cuisine %><h2>
    <p><%= f.text_field :cuisine %></p>

    <h2><%= f.label :price_range %><h2>
    <p><%= f.text_field :price_range %></p>

    <h2><%= f.label :description %><h2>
    <p><%= f.text_field :description %></p>

    <h2><%= f.label :image %><h2>
    <p><%= f.text_field :image %></p>

    <%= f.submit %>

<% end %>