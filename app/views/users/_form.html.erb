<div class="new_user_form">
<%= form_with model: @user, local: true do |f| %>

    <h3><%= f.label :name %></h3>
    <p><%= f.text_field :name %></p>

    <h3><%= f.label :email %></h3>
    <p><%= f.email_field :email %></p>

    <h3><%= f.label :phone %></h3>
    <p><%= f.text_field :phone %></p>

    <h3><%= f.label :password %></h3>
    <p><%= f.password_field :password %></p>

    <h3><%= f.label :password_confirmation %></h3>
    <p><%= f.password_field :password_confirmation %></p>
    
    <h3><%= f.label :image, 'Profile Photo' %></h3>
    <p><%= f.url_field :image %></p>
    

 <%= f.submit %>


<% end %>
</div>